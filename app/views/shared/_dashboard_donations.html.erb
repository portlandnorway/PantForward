<div class="container">
  <div class="row" id='donations'>
    <% if current_user.collections.any? %>
      <% current_user.collections.each do |donation| %>
        <%= render "shared/card_donator", element: donation %>
      <% end %>
    <% else %>
      <p> You have no donations out! Click the pluss icon to donate.</p>
    <% end %>
  </div>
</div>
