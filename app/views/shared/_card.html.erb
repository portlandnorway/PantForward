<div class="col-sm-12 col-md-6 col-lg-4">
  <div class="card-collect card-trigger" id="btn-<%= element.id %>" data-modal="myModal_<%= element.id %>">
    <div>
      <img class="avatar-large" alt="avatar-large" src="https://i.pravatar.cc/200"/>
    </div>
    <div>
      <div class="card-collect-infos">
        <h3><%= "#{element.user.first_name} donates #{element.tip} kr"%></h3>
        <p><%= element.address %></p>
      </div>
    </div>
  </div>
</div>
