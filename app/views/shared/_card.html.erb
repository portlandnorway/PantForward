<div class="col-sm-12 col-md-6 col-lg-4">
  <div class="card-collect">
    <div>
      <% if element.donator.avatar.attached? %>
        <%= cl_image_tag element.donator.avatar.key, class:"avatar-large", alt:"avatar-large" %>
      <% else %>
        <img class="avatar-large" alt="avatar-large" src="https://i.pravatar.cc/200"/>
      <% end %>
    </div>
    <div>
      <%= link_to collection_path(element[:id]), {:class=>"card-collect-infos"} do %>
        <h3><%= "#{element.donator.first_name} donates #{element.tip} kr"%></h3>
        <p><%= element.address %></p>
      <% end %>
    </div>
  </div>
</div>
